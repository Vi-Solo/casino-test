{
  "name": "nest-ddd-typeorm-postgres",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/main.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "start:prod": "node -r tsconfig-paths/register dist/main.js",
    "type-check": "tsc --noEmit",
    "lint": "echo \"No linter configured\"",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down -v",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/infra/typeorm/data-source.ts migration:generate src/migrations/Init",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/infra/typeorm/data-source.ts migration:run",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/infra/typeorm/data-source.ts migration:revert",
    "seed": "ts-node ./src/infra/typeorm/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/typeorm": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.0",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
